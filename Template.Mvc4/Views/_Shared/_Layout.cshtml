<!DOCTYPE html>
<html lang="en">
<head>
  <title>Campaign Tracker - a software singularity effort</title>
  @Html.Partial("_Head")
</head>
<body>
  <header>
    @Html.Partial("_Header")
  </header>
  <div class="xtra">
    <div class="container" id="body">
      @RenderSection("featured", false)
      <div class="content">
        @RenderBody()
      </div>
    </div>
    <p class="debug">
    Controller: @ViewContext.RouteData.Values["controller"] <br />
    Action: @ViewContext.RouteData.Values["action"] <br />
    Area: @ViewContext.RouteData.DataTokens["area"] 
</p>
  </div>
  <footer>
    @Html.Partial("_Footer")
  </footer>
  @*@Html.Partial("_DevTabs")*@
  <div id="script-wrapper">
    @Html.Partial("_Scripts")
    @RenderSection("pageScripts", false)
  </div>
</body>
</html>
