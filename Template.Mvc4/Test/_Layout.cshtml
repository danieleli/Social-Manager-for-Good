<!DOCTYPE html>
<html>
<head>
  <title>@Page.Title</title>
  <link rel="stylesheet" href="/content/lib/qunit/qunit.css " />
  <script type="text/javascript" src="/Content/lib/jquery/jquery-1.7.1.min.js"></script>
  <script type="text/javascript" src="/Content/lib/qunit/qunit.js"></script>
  @RenderSection("Javascript", false)
  @* Tests are written in the body. *@
  @RenderBody()
</head>
<body>
  <h1 id="qunit-header">
    @(Page.Title ?? "QUnit tests")
  </h1>
  <h2 id="qunit-banner"></h2>
  <h2 id="qunit-userAgent"></h2>
  <ol id="qunit-tests"></ol>
  <p>
    <a href="/test/index">Back to tests</a>
    <br />
    <a href="http://docs.jquery.com/QUnit">QUnit Homepage</a>
  </p>

  <script src="/Views/_Scripts/ko-protected-observable.js" type="text/javascript"></script>
  <script src="/Views/_Scripts/CrudHelpers.js" type="text/javascript"></script>
  <script src="/Views/_Scripts/date.js" type="text/javascript"></script>
  <script src="/Content/lib/knockout/knockout-1.3.0beta.js" type="text/javascript"></script>
  <script src="/Content/lib/knockout/knockout-mapping.js" type="text/javascript"></script>
</body>
</html>
